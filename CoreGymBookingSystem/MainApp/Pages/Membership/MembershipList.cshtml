@page
@model MainApp.Pages.Membership.MembershipListModel
@{
}
@foreach (var m in Model.Memberships)
{
    <div class="card">
        <h2>@m.Name</h2>
        <p>@m.Description</p>
        <h3>@m.Price kr</h3>
        <a asp-page="MembershipDetails" asp-route-id="@m.Id" class="btn btn-primary">Choose</a>
    </div>
}
